{
  "compilerOptions": {
    "moduleResolution": "node",
    "module": "ES2015",
    "target": "ES5",
    "lib": [ "dom", "es6", "es2015.collection" ],
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strictNullChecks": false,
    "noImplicitThis": true,
    "experimentalDecorators": true
  }
}
